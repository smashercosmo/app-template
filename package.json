{
  "name": "app-template",
  "version": "1.0.0",
  "description": "React web app template",
  "main": "index.js",
  "scripts": {
    "build:dll": "webpack --config ./webpack/webpack.dll.config.js",
    "build": "NODE_ENV=production webpack --progress --config ./webpack/webpack.production.config.js",
    "start": "NODE_ENV=development webpack --progress --config ./webpack/webpack.development.config.js",
    "serve": "npm run build && http-server ./dist -p 8000",
    "prestart": "npm run build:dll",
    "prettier": "prettier --write \"{src,webpack}/**/*.{js,ts,tsx,css}\"",
    "lint:all": "npm run lint:js && npm run lint:css && npm run lint:types",
    "lint:js": "eslint \"./**/*.tsx\"",
    "lint:css": "stylelint ./src/**/*.css",
    "lint:types": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smashercosmo/app-template.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/smashercosmo/app-template/issues"
  },
  "homepage": "https://github.com/smashercosmo/app-template#readme",
  "devDependencies": {
    "@babel/cli": "7.7.0",
    "@babel/core": "7.7.2",
    "@babel/preset-modules": "0.1.0",
    "@babel/preset-react": "7.7.0",
    "@babel/preset-typescript": "7.7.2",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "@types/webpack-env": "1.14.1",
    "@typescript-eslint/eslint-plugin": "2.7.0",
    "@typescript-eslint/parser": "2.7.0",
    "add-asset-html-webpack-plugin": "3.1.3",
    "app-root-path": "3.0.0",
    "autoprefixer": "9.7.1",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.0.5",
    "css-loader": "3.2.0",
    "cssnano": "4.1.10",
    "eslint": "6.6.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.5.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-import-helpers": "1.0.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.2.0",
    "html-webpack-plugin": "4.0.0-beta.11",
    "http-server": "0.11.2",
    "mini-css-extract-plugin": "0.8.0",
    "postcss-custom-media": "7.0.8",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.2.1",
    "prettier": "1.19.1",
    "size-plugin": "2.0.1",
    "style-loader": "1.0.0",
    "stylelint": "11.1.1",
    "stylelint-config-standard": "19.0.0",
    "typescript": "3.7.2",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-plugin-serve": "0.12.1"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "react": "16.11.0",
    "react-dom": "16.11.0"
  }
}
